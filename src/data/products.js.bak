export const allProducts = images.map((image, index) => {
  const productId = index + 1;
  return {
    id: productId,
    name: `Producto de Cuidado Facial ${productId}`,
    brand: brands[productId % brands.length],
    line: lines[productId % lines.length],
    price: parseFloat((Math.random() * (80 - 20) + 20).toFixed(2)),
    image: image,
  };
});
